        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.user.mapper.RoleMapper">

    <resultMap id="BaseResultMap" type="com.example.user.domain.Role">
        <id property="role_id" column="role_id" jdbcType="BIGINT"/>
